generator client {
    provider ="prisma-client-js"
}

datasource db{
    provider = "sqlite"
    url = "file:./dev.db"
}



model Employee{
id        Int    @id @default(autoincrement())
name      String
email     String   @unique
password  String?
mobileNo  String   @unique
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}